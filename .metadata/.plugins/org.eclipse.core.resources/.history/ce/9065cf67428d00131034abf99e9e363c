package com.sp.mall;

public class Store {
	private String store;
	private String address;
	private int phoneNumber;
	private int horary;
	private String webSite;
	private String email;
	public Store(String n,String d,int t,int h,String w, String e){
		this.store = n;
		this.address=d;
		this.phoneNumber=t;
		this.horary = h;
		this.webSite=w;
		this.email=e;
	}
	
	public String getStore(){
		return this.store.toString();
	}
	
	public String getAdress(){
		return this.address.toString();
	}
	
	public int getPhoneNumber(){
		return  this.phoneNumber;
	}
	public int getHorary(){
		return  this.horary;
	}
	public int phoneNumbre(){
		return  this.phoneNumber;
	}
	
	

}
